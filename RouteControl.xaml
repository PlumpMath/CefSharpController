<UserControl x:Class="Cliver.CefSharpController.RouteControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Cliver.CefSharpController"
             mc:Ignorable="d" 
             d:DesignHeight="300" >
    <Grid>
        <StackPanel Orientation="Vertical" Width="100" >
            <Label>State:</Label>
            <TextBox Name="state" MaxLines="1" IsEnabled="False">csdfsff</TextBox>
            <Label Height="auto" Width="auto" >Xml:</Label>
            <TextBox Name="xml" MinHeight="227" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></TextBox>
        </StackPanel>

    </Grid>
</UserControl>
