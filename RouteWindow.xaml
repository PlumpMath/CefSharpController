<Window x:Class="Cliver.CefSharpController.RouteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cliver.CefSharpController"
        mc:Ignorable="d"
        Title="RouteWindow" Height="300" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
        <Label>State:</Label>
            <TextBox Name="State" MaxLines="1" IsEnabled="False">csdfsff</TextBox>
            <Label Height="auto" Width="auto" >Xml:</Label>
            <TextBox Name="Xml" Height="197" HorizontalAlignment="Stretch"></TextBox>
        </StackPanel>
    </Grid>
</Window>
